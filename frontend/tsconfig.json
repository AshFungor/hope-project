{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["DOM", "ES2020"],
		"module": "ESNext",
		"jsx": "react-jsx",
		"moduleResolution": "Node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"paths": {
			"@app/*": ["./src/*"]
		}
	}
}
